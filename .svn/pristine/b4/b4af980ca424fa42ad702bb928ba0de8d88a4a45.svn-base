package com.system.you.review.web.beans.view;

import com.system.you.review.core.PopularTags;

public class UserInterestViewBean {

	public String getItemId() {
		return itemId;
	}

	public void setItemId(String itemId) {
		this.itemId = itemId;
	}

	public UserViewBean getUser() {
		return user;
	}

	public void setUser(UserViewBean user) {
		this.user = user;
	}

	public PopularTags getTags() {
		return tags;
	}

	public void setTags(PopularTags tags) {
		this.tags = tags;
	}

	public int getToalReviews() {
		return toalReviews;
	}

	public void setToalReviews(int toalReviews) {
		this.toalReviews = toalReviews;
	}

	private String itemId;
	
	private UserViewBean user;

	private PopularTags tags;
	
	private int toalReviews;
}
