package com.system.you.review.web.beans.view;

public class NameValuePair {

	public NameValuePair(String name, String value) {
		this.name = name;
		this.id = value;
	}

	public String getName() {
		return name;
	}

	public void setName(String label) {
		this.name = label;
	}

	public String getId() {
		return id;
	}

	public void setId(String value) {
		this.id = value;
	}

	private String name;
	private String id;
}
